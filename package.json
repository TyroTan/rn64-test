{
  "name": "rn64test",
  "version": "0.5.13",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator \"iPhone 11 Pro Max\"",
    "iosSE": "npx react-native run-ios --simulator=\"iPhone SE (2nd generation)\"",
    "iosIpad": "npx react-native run-ios --simulator=\"iPad Pro (12.9-inch) (5th generation)\"",
    "dontdothis-dangerouslyRebuild": "rm -rf node_modules && rm package-lock.json && cd ios && rm -rf Pods && rm Podfile.lock  && cd .. && npm i && cd ios && npx pod-install && cd .. && npm run ios",
    "prerebuildIOS": "rm -rf ~/Library/Developer/Xcode/DerivedData",
    "rebuildIOS": "rm -rf node_modules && cd ios && rm -rf Pods && cd .. && npm i && cd ios && npx pod-install && cd ..",
    "openFolderAAB": "open android/app/build/outputs/bundle/release",
    "buildaab": "cd android && ./gradlew bundleRelease && cd ..",
    "postbuildaab": "npm run openFolderAAB",
    "publishtest": "expo publish --release-channel test",
    "publishprod": "expo publish --release-channel prod",
    "testdeeplink": "adb shell am start -a android.intent.action.VIEW -d \"rn64test://--/merchant-deeplink/1\"",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --watchAll"
  },
